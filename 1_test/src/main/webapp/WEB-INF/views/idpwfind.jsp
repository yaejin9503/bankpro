function idfind(){
	var name = $("#name").val();
	var email = $("#email").val(); 

	var infomation={
		"name":name,
		"email":email
	}
	
	$.ajax({
		type:'GET',
		data:infomation,
		url:'idfind',
		success: function(resp){
			if(resp==''){
				alert("해당 아이디가 존재하지 않음");
				return false; 
			}
			else if(resp!=''){
				alert("아이디는 "+ resp+" 입니다."); 
			}
		}
	})
}

function pwfind(){
	var custid = $("#custid").val();
	var name = $("#name2").val();
	var email = $("#email2").val(); 
	
	console.log(name +" "+email+" "+custid); 
	
	var infomation={
		"custid":custid,
		"name":name,
		"email":email
	}
	
	$.ajax({
		type:'GET',
		data:infomation,
		url:'pwfind',
		success: function(resp){
			if(resp==''){
				alert("해당 비밀번호가 존재하지 않음");
				return false; 
			}
			else if(resp!=''){
				alert("비밀번호는 "+ resp+" 입니다."); 
			}
		}
	})
	
}
